{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Edit Tsconfig",
      "program": "${workspaceFolder}/bin/tsconfig-to-prod.js",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Restore Tsconfig",
      "program": "${workspaceFolder}/bin/tsconfig-to-dev.js",
      "sourceMaps": true,
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    }
  ]
}
